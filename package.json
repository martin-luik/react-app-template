{
  "name": "react-app-template",
  "version": "1.0.0",
  "description": "React Application Template",
  "repository": {
    "type": "git",
    "url": "https://github.com/vergil465/react-app-template.git"
  },
  "main": "index.js",
  "scripts": {
    "start-dev": "webpack-dev-server -d --progress --colors --config webpack.config.dev.js",
    "start": "npm run build && node server.js",
    "build": "webpack -p --config webpack.config.prod.js --hide-modules",
    "transpile": "babel src -d dist --copy-files",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-watch-all": "jest --watchAll",
    "test-coverage": "jest --coverage --colors",
    "lint": "eslint src --fix"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer",
      "jest-emotion"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg)$": "<rootDir>src/emptyMock.js"
    }
  },
  "keywords": [
    "React",
    "Application",
    "Template"
  ],
  "author": "Martin Luik",
  "license": "ISC",
  "dependencies": {
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.0.1",
    "react-simple-responsive-framework": "1.6.7",
    "emotion": "10.0.17",
    "redux": "4.0.4",
    "redux-promise-middleware": "6.1.1",
    "redux-thunk": "2.3.0",
    "express": "4.17.1",
    "axios": "0.19.0",
    "prop-types": "15.7.2",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "svgxuse": "1.2.6"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "@babel/polyfill": "7.6.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "classnames": "2.2.6",
    "css-loader": "1.0.1",
    "enzyme-to-json": "3.4.0",
    "eslint": "6.2.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "file-loader": "4.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.9.0",
    "jest-emotion": "10.0.17",
    "redux-mock-store": "1.5.3",
    "moment": "2.24.0",
    "node-sass": "4.12.0",
    "sass-loader": "7.3.1",
    "style-loader": "1.0.0",
    "url-loader": "2.1.0",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.8",
    "webpack-dev-server": "3.8.0",
    "thunk": "0.0.1",
    "hard-source-webpack-plugin": "0.13.1",
    "mini-css-extract-plugin": "0.8.0"
  }
}
