{
  "name": "react-app-template",
  "version": "1.0.0",
  "description": "React Application Template",
  "repository": {
    "type": "git",
    "url": "https://github.com/vergil465/react-app-template.git"
  },
  "main": "index.js",
  "scripts": {
    "start-dev": "nodemon --exec \"webpack-dev-server -d --progress --colors --config webpack.config.dev.js\"",
    "start": "node server.js",
    "build": "webpack -p --config webpack.config.prod.js --hide-modules",
    "transpile": "babel src -d dist --copy-files",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-watch-all": "jest --watchAll",
    "test-coverage": "jest --coverage --colors",
    "lint": "eslint src --fix"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer",
      "jest-emotion"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg)$": "<rootDir>src/emptyMock.js"
    }
  },
  "keywords": [
    "React",
    "Application",
    "Template"
  ],
  "author": "Martin Luik",
  "license": "ISC",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-simple-responsive-framework": "^1.7.0",
    "emotion": "^10.0.27",
    "redux": "^4.0.5",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "express": "^4.17.1",
    "axios": "^0.19.0",
    "prop-types": "^15.7.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "svgxuse": "^1.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "css-loader": "^3.4.0",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.27",
    "redux-mock-store": "^1.5.4",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "thunk": "^0.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "terser-webpack-plugin": "^2.3.1",
    "nodemon": "^2.0.2",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
